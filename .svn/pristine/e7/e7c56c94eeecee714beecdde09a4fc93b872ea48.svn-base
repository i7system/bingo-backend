package br.com.bingo.funcoes;

import java.text.SimpleDateFormat;
import java.util.Date;

public class DataHora {
	
	public static String DataHoraMinutoSegundo() {
		SimpleDateFormat d =  new SimpleDateFormat("yyyy-MM-dd-HH:mm:ss.SSS");  
		System.out.println(d.format(new Date())); 
		
		String datahora = d.format(new Date());
		
		return datahora;

	}

}
